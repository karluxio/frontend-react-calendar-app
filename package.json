{
  "name": "calendar",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test:dependencies": "pnpm i -D jest babel-jest @babel/preset-env @babel/preset-react @testing-library/react @types/jest jest-environment-jsdom whatwg-fetch",
    "test:create:files": "touch babel.config.cjs jest.config.cjs jest.setup.js && mkdir -p tests/mocks && touch tests/mocks/styleMock.js && echo '// snippet --> config-babel' > babel.config.cjs && echo '// snippet --> config-jest' > jest.config.cjs && echo '// snippet --> config-jest-setup' > jest.setup.js && echo 'module.exports = {}' > tests/mocks/styleMock.js",
    "config:test": "pnpm test:dependencies && pnpm test:create:files",
    "test": "jest --watchAll"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^1.4.0",
    "date-fns": "^2.30.0",
    "react": "^18.2.0",
    "react-big-calendar": "^1.8.1",
    "react-datepicker": "^4.14.0",
    "react-dom": "^18.2.0",
    "react-modal": "^3.16.1",
    "react-redux": "^8.1.0",
    "react-router-dom": "^6.13.0",
    "sweetalert2": "6.6.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-react": "^7.22.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.3",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "@vitejs/plugin-react": "^4.0.0",
    "babel-jest": "^29.6.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.42.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.5",
    "git-commit-msg-linter": "^5.0.4",
    "jest": "^29.6.1",
    "jest-environment-jsdom": "^29.6.1",
    "setimmediate": "^1.0.5",
    "vite": "^4.3.9",
    "whatwg-fetch": "^3.6.17"
  }
}